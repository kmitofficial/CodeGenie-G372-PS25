{
	"compilerOptions": {
	  "target": "ES2022",
	  "module": "ESNext",
	  "lib": ["ES2022"],
	  "moduleResolution": "Node",
	  "outDir": "dist",
	  "rootDir": "src",
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true
	},
	"include": ["src"],
	"exclude": ["node_modules", "dist"]
  }
  